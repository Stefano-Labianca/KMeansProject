    "@storybook/svelte": "7.0.24",
    "@storybook/sveltekit": "7.0.24",
    "@storybook/testing-library": "0.0.14-next.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
